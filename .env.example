# Discord Bot Token (必須)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# 管理者ユーザーID (カンマ区切りで複数指定可能)
ADMIN_USER_IDS=123456789012345678,987654321098765432

# 管理者ロール名 (カンマ区切りで複数指定可能)
ADMIN_ROLES=Administrator,Moderator,Staff

# Welcome機能のチャンネルID (新規メンバー歓迎メッセージを送信するチャンネル)
WELCOME_CHANNEL_ID=1234567890123456789

# 毎日通知機能のチャンネルID (新着数式を通知するチャンネル)
DAILY_NOTIFICATION_CHANNEL_ID=1234567890123456789

# メッセージ管理用Google Apps Script API
MESSAGES_API_URL=https://script.google.com/macros/s/your_script_id_here/exec
MESSAGES_API_KEY=your_optional_api_key_here

# 数式データ管理用Google Apps Script API
FORMULAS_API_URL=https://script.google.com/macros/s/your_formulas_script_id_here/exec
FORMULAS_API_KEY=your_optional_formulas_api_key_here
